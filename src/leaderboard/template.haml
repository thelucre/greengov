.leaderboard.block.blue
	.content(v-class="show: isVisible, hide: !isVisible")
		%h1.header
			%span How everyone stacks up
		%ul.organizations(v-class="show: isVisible, hide: !isVisible")
			%li.list-header
				%span.rank Rank
				%span.name Agency Name
				%span.pass-fail Pass/Fail
				%span.reduction Reduction
			%li(v-repeat='org in organizationsWithStats')
				%org-detail(org="{{ org }}")
